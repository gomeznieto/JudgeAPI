{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "SubmissionPaths": {
    "temp": "TempSubmission"
  },
  "CompilerSettings": {
    "CppStandard": "c++11",
    "Flags": "-pedantic-errors -Wall -Wextra"
  }
}
